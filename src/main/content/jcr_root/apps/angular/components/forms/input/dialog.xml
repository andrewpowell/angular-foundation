<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="475"
    title="Input Details"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Panel"
        xtype="panel">
        <items jcr:primaryType="cq:WidgetCollection">
            <requiredItems
                jcr:primaryType="cq:Widget"
                title="Required Items"
                xtype="dialogfieldset">
                <items jcr:primaryType="cq:WidgetCollection">
                    <type
                        jcr:primaryType="cq:Widget"
                        allowBlank="false"
                        defaultValue="text"
                        fieldLabel="Input Type"
                        name="./type"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <button
                                jcr:primaryType="nt:unstructured"
                                text="button"
                                value="button"/>
                            <color
                                jcr:primaryType="nt:unstructured"
                                text="color"
                                value="color"/>
                            <date
                                jcr:primaryType="nt:unstructured"
                                text="date"
                                value="date"/>
                            <datetime
                                jcr:primaryType="nt:unstructured"
                                text="datetime"
                                value="datetime"/>
                            <datetime-local
                                jcr:primaryType="nt:unstructured"
                                text="datetime-local"
                                value="datetime-local"/>
                            <file
                                jcr:primaryType="nt:unstructured"
                                text="file"
                                value="file"/>
                            <hidden
                                jcr:primaryType="nt:unstructured"
                                text="hidden"
                                value="hidden"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                text="image"
                                value="image"/>
                            <month
                                jcr:primaryType="nt:unstructured"
                                text="month"
                                value="month"/>
                            <password
                                jcr:primaryType="nt:unstructured"
                                text="password"
                                value="password"/>
                            <range
                                jcr:primaryType="nt:unstructured"
                                text="range"
                                value="range"/>
                            <reset
                                jcr:primaryType="nt:unstructured"
                                text="reset"
                                value="reset"/>
                            <search
                                jcr:primaryType="nt:unstructured"
                                text="search"
                                value="search"/>
                            <submit
                                jcr:primaryType="nt:unstructured"
                                text="submit"
                                value="submit"/>
                            <tel
                                jcr:primaryType="nt:unstructured"
                                text="tel"
                                value="tel"/>
                            <time
                                jcr:primaryType="nt:unstructured"
                                text="time"
                                value="time"/>
                            <week
                                jcr:primaryType="nt:unstructured"
                                text="week"
                                value="week"/>
                        </options>
                    </type>
                    <name
                        jcr:primaryType="nt:unstructured"
                        allowBlank="false"
                        fieldLabel="name"
                        name="./name"
                        validateOnBlur="true"
                        xtype="textfield"/>
                </items>
            </requiredItems>
            <optionalItems
                jcr:primaryType="cq:Widget"
                collapsed="{Boolean}true"
                collapsible="{Boolean}true"
                title="Optional Items"
                xtype="dialogfieldset">
                <items jcr:primaryType="cq:WidgetCollection">
                    <ng-model
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="ng-model"
                        name="./ng-model"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <value
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="Field Value"
                        name="./value"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <label
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="Display Label"
                        name="./label"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <required
                        jcr:primaryType="cq:Widget"
                        allowBlank="true"
                        defaultValue="false"
                        fieldLabel="required"
                        name="./required"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <one
                                jcr:primaryType="nt:unstructured"
                                text="true"
                                value="true"/>
                            <two
                                jcr:primaryType="nt:unstructured"
                                text="false"
                                value="false"/>
                        </options>
                    </required>
                    <ng-required
                        jcr:primaryType="cq:Widget"
                        allowBlank="true"
                        defaultValue="false"
                        fieldLabel="ng-required"
                        name="./ng-Required"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <one
                                jcr:primaryType="nt:unstructured"
                                text="true"
                                value="true"/>
                            <two
                                jcr:primaryType="nt:unstructured"
                                text="false"
                                value="false"/>
                        </options>
                    </ng-required>
                    <ng-minLength
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="ng-minlength"
                        name="./ng-minLength"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <ng-maxLength
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="ng-maxlength"
                        name="./ng-maxLength"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <ng-pattern
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="ng-pattern"
                        name="./ng-pattern"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <ng-change
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="ng-change"
                        name="./ng-change"
                        validateOnBlur="true"
                        xtype="textfield"/>
                    <class
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="CSS Class name"
                        name="./class"
                        validateOnBlur="true"
                        xtype="textfield"/>
                </items>
            </optionalItems>
        </items>
    </items>
</jcr:root>
