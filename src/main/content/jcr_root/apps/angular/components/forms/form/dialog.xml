<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="150"
    title="Controller Detail"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Panel"
        border="false"
        xtype="panel">
        <items jcr:primaryType="cq:WidgetCollection">
            <controllerName
                jcr:primaryType="nt:unstructured"
                fieldLabel="ng-controller"
                name="./controllerName"
                validateOnBlur="true"
                xtype="textfield"/>
            <ngSubmit
                jcr:primaryType="nt:unstructured"
                fieldLabel="ng-submit"
                name="./ngSubmit"
                validateOnBlur="true"
                xtype="textfield"/>
            <ngClick
                jcr:primaryType="nt:unstructured"
                fieldLabel="ng-click"
                name="./ngClick"
                validateOnBlur="true"
                xtype="textfield"/>
        </items>
    </items>
</jcr:root>
